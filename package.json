{
  "name": "studio-ghibli-service",
  "version": "0.0.2",
  "description": "This is a Node.js API with a scrapy module to fetch data from: imdb, rottentomatoes and metacritic",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "SLS_DEBUG=* && node ./node_modules/.bin/serverless offline",
    "build-doc": "apidoc -i src/ -o docs",
    "lint": "eslint --format codeframe src",
    "test": "jest -u --silent test",
    "coveralls": "jest --silent --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malaquiasdev/studio-ghibli-service.git"
  },
  "author": "Mateus Malaquias <mateusmalaquiasdev@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malaquiasdev/studio-ghibli-service/issues"
  },
  "homepage": "https://github.com/malaquiasdev/studio-ghibli-service#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "dynamoose": "^1.11.1",
    "serverless-dotenv-plugin": "^2.3.2",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "apidoc": "^0.20.1",
    "aws-sdk": "^2.650.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.4",
    "prettier": "^2.0.2",
    "serverless": "^1.67.0",
    "serverless-offline": "^6.1.2"
  }
}
